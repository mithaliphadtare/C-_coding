#include<iostream>
using namespace std;
int main(){
    float a,b,ans;
    char op;
    cout<<"Enter two Numbers:\n";
    cin>>a>>b;
    cout<<"Enter operations(+,-,*,/):\n";
    cin>>op;
    switch(op){
    case '+':
    ans=a+b;
    cout<<ans;
    break;

    case '-':
    ans=a-b;
    cout<<ans;
    break;

    case '*':
    ans=a*b;
    cout<<ans;
    break;

    case '/':
    ans=a/b;
    cout<<ans;
    break;

    default:
    cout<<"Invalid operation";
    break;
    }

    return 0;

}
